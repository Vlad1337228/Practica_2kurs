        </TextBox>
        <TextBox x:Name="textBoxImgBox5" HorizontalAlignment="Left" Height="62" Margin="984,510,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" FontFamily="Verdana" FontSize="40" Foreground="#FFFDFDFD">
