            </TextBox.BorderBrush>
            <TextBox.Background>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.Background>
        </TextBox>
        <TextBox x:Name="textBoxImgBox7" HorizontalAlignment="Left" Height="62" Margin="275,760,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" FontFamily="Verdana" FontSize="40" Foreground="#FFFDFDFD">
            <TextBox.BorderBrush>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.BorderBrush>
            <TextBox.Background>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.Background>
        </TextBox>
        <TextBox x:Name="textBoxImgBox7_Copy" HorizontalAlignment="Left" Height="62" Margin="275,757,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" FontFamily="Verdana" FontSize="40" Foreground="#FFFDFDFD">
            <TextBox.BorderBrush>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.BorderBrush>
            <TextBox.Background>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.Background>
        </TextBox>
        <TextBox x:Name="textBoxImgBox8" HorizontalAlignment="Left" Height="62" Margin="985,759,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" FontFamily="Verdana" FontSize="40" Foreground="#FFFDFDFD">
            <TextBox.BorderBrush>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.BorderBrush>
            <TextBox.Background>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.Background>
        </TextBox>
        <Button x:Name="buttonExit" Content="Выход" HorizontalAlignment="Left" Margin="1456,122,0,0" VerticalAlignment="Top" Width="218" Foreground="#FFFFFEFE" FontSize="20" Click="buttonExit_Click" BorderBrush="#FF6E6E6E" BorderThickness="0,0,0,1">
            <Button.Background>
                <SolidColorBrush Color="#FFDDDDDD" Opacity="0"/>
            </Button.Background>

        </Button>

    </Grid>
</Window>
   