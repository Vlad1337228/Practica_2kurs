        </TextBox>
        <TextBox x:Name="textBoxImgBox4" HorizontalAlignment="Left" Height="62" Margin="626,514,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" FontFamily="Verdana" FontSize="40" Foreground="#FFFDFDFD">
            <TextBox.BorderBrush>
                <SolidColorBrush Color="White" Opacity="0"/>
