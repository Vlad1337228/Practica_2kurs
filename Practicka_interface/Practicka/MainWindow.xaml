        <Image x:Name="imageBox4" HorizontalAlignment="Left" Height="222" Margin="602,494,0,0" VerticalAlignment="Top" Width="356" Source="dubai.jpg" StretchDirection="DownOnly" Stretch="UniformToFill"/>
        <Image x:Name="imageBox5" HorizontalAlignment="Left" Height="222" Margin="958,494,0,0" VerticalAlignment="Top" Width="356" Source="egypet.jpg" StretchDirection="DownOnly" Stretch="UniformToFill"/>
        <Image x:Name="imageBox6" HorizontalAlignment="Left" Height="222" Margin="1314,494,0,0" VerticalAlignment="Top" Width="361" Source="russia.jpg" StretchDirection="DownOnly" Stretch="UniformToFill"/>
